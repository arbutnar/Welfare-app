{
	"compilerOptions": {
		"baseUrl": "./",
		"rootDir": "./src",
		"outDir": "/home/app/static/js/",
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es6",
		"allowJs": true,
		"typeRoots": ["./src/types", "./node_modules/@types"],
	},
	"include": [
		"./src/**/*"
	],
	"watchOptions": {
		"watchFile": "dynamicPriorityPolling",
		"watchDirectory": "dynamicPriorityPolling"
	}
}